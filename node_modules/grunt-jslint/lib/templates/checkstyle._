<?xml version="1.0" encoding="utf-8"?>
<checkstyle>
  <% report.forEach(function (file) { %>
    <file name="<%= file.name %>">
      <% file.issues.forEach(function (issue) { %>
        <error
          line="<%= issue.line %>"
          column="<%= issue.character %>"
          severity="<%=  issue.severity %>"
          message="<%= issue.message %>"
          source="<%= issue.source %>" />
      <% }); %>
    </file>
  <% }); %>
</checkstyle>
